  printer.write(0x28);
  printer.write(0x6B);
  printer.write(0x03);
  printer.write(0x00);
  printer.write(0x31);
  printer.write(0x43);
  printer.write(0x08);
  printer.write(0x1D);
  printer.write(0x28);
  printer.write(0x6B);
  printer.write(0x03);
  printer.write(0x00);
  printer.write(0x31);
  printer.write(0x45);
  printer.write(0x30);
  printer.write(0x1D);
  printer.write(0x28);
  printer.write(0x6B);
